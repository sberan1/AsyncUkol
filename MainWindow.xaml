<Window x:Class="AsyncUkol.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncUkol"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300">
    <StackPanel>
        <Label Content="Vyber co hledas" VerticalAlignment="Center" VerticalContentAlignment="Center" Margin=" 0 10 0 0"/>
        <ComboBox Margin="0 10 0 0" x:Name="Combobox"/>
        <Label Content="Max cislo?" Margin=" 0 10 0 0"/>
        <ComboBox x:Name="maxCislo" Margin=" 0 10 0 0"/>
        <Button Content="Vybrano, Sefe" Click="VybranoSefe_Click" Margin=" 0 10 0 0"/>
        <ListBox x:Name="Listbox" Margin="0 20 0 0" Height="250"/>
    </StackPanel>
</Window>
